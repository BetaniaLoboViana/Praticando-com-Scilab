// Criando uma matriz
A = [1 2 3; 4 5 6; 7 8 9];
B = [2 2 2; 3 3 3; 4 4 4];

// Operações entre matrizes
soma = A + B;
produto = A * B;   // Multiplicação de matrizes
transposta = A';   // Transposta
inversa = inv(A);  // Inversa (se for invertível)
determinante = det(A); // Determinante

disp(determinante);
